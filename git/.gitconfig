[color]
  ui = auto
[diff]
  tool = vimdiff
[difftool]
  prompt = false
[merge]
  tool = vimdiff
  conflictstyle = diff3
[mergetool]
  prompt = false
[core]
  editor = vim
  excludesfile = ~/.gitignore_global
[pull]
  ff = only
[include]
  path = ~/.gitconfigUser

